{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "sourceMap": true,
      "typeRoots": ["./src/types", "./node_modules/@types"],
   }
}